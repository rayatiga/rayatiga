{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**", "**/res/apache2-benchmark", "**/res/apache2-wordpress-config", "**/res/cdn", "**/res/documentation", "**/res/pawonkos799", "**/res/php-ini-configuration", "**/res/sultan-kautsar", "**/res/wordpress-extractor", "**/res/wordpress-installer"],
    "redirects": [
      {
        "source": "/hiring",
        "destination": "/career",
        "type": 301
      },
      {
        "source": "/sultankautsar",
        "destination": "https://sultankautsar.com/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png|css|js|eot|otf|ttf|ttc|woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ]
  }
}
