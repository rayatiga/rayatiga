{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/hiring",
        "destination": "/career.html",
        "type": 301
      },
      {
        "source": "/legacy",
        "destination": "https://legacy.rayatiga.com",
        "type": 301
      },
      {
        "source": "/privacy-policy",
        "destination": "https://legacy.rayatiga.com/#privacy-policy",
        "type": 301
      },
      {
        "source": "/cdn",
        "destination": "https://cdn.rayatiga.com",
        "type": 301
      },
      {
        "source": "/cloud",
        "destination": "https://cloud.rayatiga.com",
        "type": 301
      },
      {
        "source": "/sultankautsar",
        "destination": "https://sultankautsar.com",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png|css|js|eot|otf|ttf|ttc|woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
