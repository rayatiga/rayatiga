{
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": ["/web/", "/web/hosting"],
        "destination": "/res/maintenance/",
        "type": 301
      },
      {
        "source": ["/web/pricing/", "/web/create/"],
        "destination": "/res/maintenance/",
        "type": 301
      },
      {
        "source": "/hiring/",
        "destination": "/career/",
        "type": 301
      },
      {
        "source": "/sultankautsar/",
        "destination": "https://sultankautsar.com/",
        "type": 301
      }
    ],
    "headers": [
      {
        "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2|font.css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png|css|js|eot|otf|ttf|ttc|woff|woff2)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ]
  }
}
